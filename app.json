{
  "expo": {
    "name": "EduMedia",
    "slug": "EduMedia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/eduMediaLogo.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/eduMediaSplashPage.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryAddUsageDescription": "This app would like to save images to your device",
        "NSPhotoLibraryUsageDescription": "This app would like to save images to your device"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow EduMedia access to your library.",
          "savePhotosPermission": "Allow EduMedia to save photos to library.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  }
}